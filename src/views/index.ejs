<%- include('partials/header') %>

    <h1>My Homepage</h1>
    <h3>List of Users</h3>
    <table border="1">
      <thead>
        <tr>
          <th>Username</th>
          <th>Password</th>
          <th>Email Address</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.password %></td>
            <td><%= user.email %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>

    <a href="/register">Register an account</a>

  <%- include('partials/footer') %>